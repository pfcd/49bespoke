{
  "name": "woocommerce-carousel-for-divi",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "divi-scripts": "file:./dev/divi-scripts-modified",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-id-swiper": "^1.6.9",
    "react-star-ratings": "^2.3.0"
  },
  "scripts": {
    "start": "divi-scripts start",
    "build": "divi-scripts build",
    "update-build": "yarn upgrade \"file:./dev/divi-scripts-modified\" && divi-scripts build",
